.con{
  background-color: #f5f5f5;
  height: 100vh;
  width: 100%;
  font-size: 30rpx;
  color: #777777;
  letter-spacing: 5rpx;
}
.header{
  line-height: 100rpx;
  font-size: 50rpx;
  font-weight: 700;
  text-align: center;
  margin: 0 0 60rpx 0;
  height: 100rpx;
  border: 1px solid #f5f5f5;
}
button{
  display: flex;
  width: 100%;
  line-height: 75rpx;
  margin: 0 0 10rpx 0;
  background-color: white;
  height: 75rpx;
  font-size: 40rpx;
}